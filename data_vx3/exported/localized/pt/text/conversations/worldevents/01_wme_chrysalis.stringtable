    <Entry>
      <ID>63</ID>
      <DefaultText>Você não sabe o que é a nuvem, mas não parece segura e parece estar se aproximando...</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>64</ID>
      <DefaultText>Ataque com a magia geada.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>65</ID>
      <DefaultText>Ataque com a magia Raio.</DefaultText>
      <FemaleText />
    </Entry>
