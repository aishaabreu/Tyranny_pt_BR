<StringTableFile>
  <Name>conversations\08_vendrienswell\0820_bs_binders</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>6</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"Por favor, Vinculador do Destino, converse com eles!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"Eu não viajei para o [url=glossary:stone sea]Mar de Pedra[/url] para ser a da mulher-fera próxima refeição..."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Estaremos a caminho em breve, teremos mais cuidado da próxima vez."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"Eu gostaria que não tivesse que vir ao derramamento de sangue, mas pelo menos estamos seguros."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>"Eu tinha certeza que minha cabeça estaria no estômago daquela fera."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>
      <DefaultText>"Estou grato por termos conseguido resolver isso pacificamente. Obrigado, Vinculador do Destino."</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
