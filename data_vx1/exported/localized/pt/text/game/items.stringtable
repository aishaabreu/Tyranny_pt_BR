      <ID>1932</ID>
      <DefaultText>Encore de Songbird</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1933</ID>
      <DefaultText>Ecos do Tempestade</DefaultText>
      <FemaleText />
    </Entry>
