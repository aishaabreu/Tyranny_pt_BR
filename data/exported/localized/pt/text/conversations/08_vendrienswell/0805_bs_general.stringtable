<StringTableFile>
  <Name>conversations\08_vendrienswell\0805_bs_general</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>7</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"O Vinculador do Destino! Defenda a ponte!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"Eles atravessaram o rio! Leva eles derrubam!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Cuidado! O inimigo pode atacar novamente a qualquer momento."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"Pensei que estava morto com certeza. Vá em frente, Vinculador do Destino. Não tenho mais utilidade para você agora."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>"Excelente trabalho, Vinculador do Destino. Vamos segurar a ponte contra qualquer reforço."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>
      <DefaultText>"Melhor não perder o ímpeto. Empurre para a aldeia! Nós vamosEu entro assim que pudermos."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>"[url=glossary:kyros]Kyros[/url]' os açougueiros chegaram! Fugir!"</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
